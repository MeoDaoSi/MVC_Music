<div class="mt-4">
    <div class="row">
        {{#each songs}}
        <div class="col-sm-6 col-lg-4 col-card-song">
            <div class="card">
            <img src="{{this.img}}" class="card-img-top" alt="{{this.name}}">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.author}}</p>
                <a href="#" class="btn btn-primary">Music</a>
            </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>